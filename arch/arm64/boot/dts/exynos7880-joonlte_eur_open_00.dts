/dts-v1/;
#include "exynos7880-joonlte_common.dtsi"
#include "exynos7880-joonlte_eur_open_gpio_00.dtsi"
#include "exynos7880-joonlte_eur_open_battery_00.dts"
#include "exynos7880-joonlte_fingerprint-sensor_00.dtsi"

/ {
	model = "Samsung JOON LTE EUR rev00 board based on Exynos 7880";
	model_info-chip = <7880>;
	model_info-platform = "android";
	model_info-subtype = "samsung";
	model_info-hw_rev = <0>;
	model_info-hw_rev_end = <0>;
	compatible = "samsung, JOON LTE EUR OPEN 00", "samsung,exynos7880";

	hsi2c@10610000 {
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <2500000>;
		samsung,hsi2c-batcher;

		s2mpu05_pmic@66 {
			regulators {
				ldo7_reg: LDO7 {
					regulator-name = "vdd_ldo7";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <2375000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo30_reg: LDO30 {
					regulator-name = "tsp_io";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <2375000>;
					regulator-boot-on;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <3>;
				};

				ldo32_reg: LDO32 {
					regulator-name = "vdd_ldo32";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo34_reg: LDO34 {
					regulator-name = "vdd_ldo34";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <3375000>;
					regulator-always-on;
					regulator-ramp-delay = <12000>;
					regulator-initial-mode = <1>;
				};

				ldo35_reg: LDO35 {
					regulator-name = "VDD_MOTOR_3P0";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <3300000>;
					regulator-ramp-delay = <12000>;
				};
			};
		};
	};

	hsi2c_6: hsi2c@138F0000 {
		gpios = <&gpf2 1 0 &gpf2 0 0>;
		status = "okay";
		clock-frequency = <300000>;
		sec-nfc@27 {
			compatible = "sec-nfc";
			reg = <0x27>;
			interrupts = <6 0 0>;
			interrupt-parent = <&gpa2>;

			sec-nfc,ven-gpio = <&gpc2 2 0>;
			sec-nfc,firm-gpio = <&gpd4 1 0>;
			sec-nfc,irq-gpio = <&gpa2 6 0>;
			sec-nfc,pvdd_1p8-gpio = <&gpc2 3 0>;
			sec-nfc,i2c_1p8-gpio = <&gpd4 3 0>;

			pinctrl-names = "default";
			pinctrl-0 = <&nfc_firm &nfc_pd &nfc_int &nfc_vdd_en>;
		};
	};

	i2c@13850000 {
		status = "okay";
		s2mpb02_pmic@59 {
			compatible = "s2mpb02,s2mpb02mfd";
			reg = <0x59>;

			s2mpb02,wakeup;
			pinctrl-names = "default";
			pinctrl-0 = <&sub_pmic_irq>;

			regulators {

				_ldo17_reg: s2mpb02-ldo17 {
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <3775000>;
					regulator-always-on;
					regulator-boot-on;
				};

			};
		};
	};

	dsim_0: dsim@0x14800000 {
		gpios = <&gpd3 4 0x1>, <&gpb0 1 0x1>;
	};

	cnss_dcdc_en: cnss_dcdc_en {
		compatible = "regulator-fixed";
		regulator-name = "cnss_dcdc_en";
		startup-delay-us = <4000>;
		enable-active-high;
		gpio = <&gpd3 7 0>;
	};

	wlan {
		compatible = "qcom,cnss_sdio";
		wlan-en-gpio = <&gpd3 6 0>; /* WLAN_EN */
		wlan-host-wake = <&gpa2 2 0>; /* WLAN_HOST_WAKE */
		vdd-wlan-supply = <&cnss_dcdc_en>; /* DCDC_EN regulator */
		pinctrl-names = "default";
		pinctrl-0 = <&cnss_wlanen &cfg_wlanhostwake>;
		status = "okay";
	};

	pinctrl@139B0000 {
		cnss_wlanen: cnss-wlanen  {       /* WLAN_EN */
			samsung,pins = "gpd3-6";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
			samsung,pin-con-pdn = <3>;
			samsung,pin-pud-pdn = <0>;
			samsung,pin-val = <0>;
		};
	};

	dwmmc1@13550000 {
		status = "okay";
		num-slots = <1>;
		channel = <1>;
		fixed_voltage;
		/*clock-gate;*/
		enable-cclk-on-suspend;
		caps-control;
		supports-highspeed;
		supports-4bit;
		sd-uhs-sdr104;
		keep-power-in-suspend;
		pm-ignore-notify;
		card-detect-type-external;
		use-broken-voltage;
		use-fine-tuning;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <468000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 2 1>;
		clk_pin = "gpr2-0";
		clk_addr = "13750000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <4>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;
		slot@0 {
			reg = <0>;
			bus-width = <4>;
		};
	};

	pinctrl@139B0000 {
		tsp_1p8_en: tsp-1p8-en {
			samsung,pins = "gpb0-2";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
			samsung,pin-val = <1>;
		};
	};

	bluetooth {
		compatible = "qca,qca6174";
		gpios = <&gpd4 2 0x1	/*BT_EN*/
				&gpd5 1 0x1	/*BT_WAKE*/
				&gpa1 6 0xF	/*BT_HOST_WAKE*/ >;
		vdd-wlan-supply = <&cnss_dcdc_en>; /* DCDC_EN regulator */
		pinctrl-names = "default";
		pinctrl-0=<&bt_hostwake &bt_btwake &bt_en>;
		status = "okay";
	};

	i2c@13840000 {
		status = "okay";
		samsung,i2c-max-bus-freq = <400000>;
		touchscreen@49 {
			compatible = "stm,fts_touch";
			reg = <0x49>;
			pinctrl-names = "on_state", "off_state";
			pinctrl-0 = <&attn_irq &tsp_1p8_en>;
			pinctrl-1 = <&attn_input>;
			stm,tspid_gpio = <&gpb0 2 0>;
			stm,irq_gpio = <&gpc3 2 0>;
			stm,irq_type = <8200>;
			stm,max_coords = <1439 2559>;	/* x y */
			stm,regulator_dvdd = "vdd_ldo32";
			stm,regulator_avdd = "vdd_ldo34";
			stm,project_name = "joon", "smdk7880";
			stm,firmware_name = "tsp_stm/stm_v5583.fw";
		};
	};

	pinctrl@139B0000 {
		/* touchkey power */
		cfg_vdd_en: cfg-vdd-en {
			samsung,pins = "gpd1-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	touchkey_ldo: fixedregulator@20 {
		compatible = "regulator-fixed";
		pinctrl-names = "default";
		pinctrl-0 = <&cfg_vdd_en>;
		regulator-name = "touchkey_ldo";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpd1 6 0>;
		enable-active-high;
	};

	touchkeyi2c {
		status = "okay";
		compatible = "i2c-gpio";
		pinctrl-names = "on_i2c", "off_i2c";
		pinctrl-0 = <&cfg_gpio>;
		pinctrl-1 = <&cfg_gpio>;

		gpios = <&gpa2 4 0 /* sda */ &gpa2 5 0 /* scl */>;
		i2c-gpio,delay-us = <3>;
		i2c-gpio,timeout-mt = <100>;

		#address-cells = <1>;
		#size-cells = <0>;

		cypress@20 {
			status = "okay";
			compatible = "cypress,cypress_touchkey";
			pinctrl-names = "on_irq", "off_irq", "sub_det";
			pinctrl-0 = <&cfg_irq>;
			pinctrl-1 = <&cfg_input>;
			pinctrl-2 = <&sub_det>;
			reg = <0x20>;
			interrupts = <4 0 0>;
			interrupt-parent = <&gpa1>;
			cypress,sda-gpio = <&gpa2 4 0>;
			cypress,scl-gpio = <&gpa2 5 0>;
			cypress,irq-gpio = <&gpa1 4 1>;
			cypress,sub-det = <&gpa3 0 0>;
			cypress,ic-stabilizing-time = <150>;
			cypress,i2c-gpio = "true";
			cypress,ap-io-power = "true";
			cypress,fw_path = "cypress/cypress_zerof.fw";
			vtouch_3.3v-supply = <&touchkey_ldo>;
		};
	};

	fimc_is_sensor_2p2: fimc-is_sensor_2p2@5A {
		compatible = "samsung,sensor-module-2p2";

		pinctrl-names = "pin0", "pin1", "pin2";
		pinctrl-0 = <>;
		pinctrl-1 = <&fimc_is_flash_host &fimc_is_mclk0_in &fimc_is_mclk2_in>;
		pinctrl-2 = <&fimc_is_flash_host &fimc_is_mclk0_fn &fimc_is_mclk2_fn>;

		position = <0>;	/* rear:0. front:1 */
		id = <0>; /* bns_id */
		mclk_ch = <0>;
		sensor_i2c_ch = <0>; /* i2c 0 */
		sensor_i2c_addr = <0x5A>;

		gpio_mclk = <&gpe0 0 0x1>;
		gpio_reset = <&gpd3 5 0x1>; /* sensor reset */
		gpio_prep_reset = <&gpb0 0 0x1>; /* preprocessor reset */
		status = "okay";

		af {
			product_name = <12>; /* ak7348 */
			i2c_addr = <0x18>;
			i2c_ch = <1>; /* i2c 2 */
	};

		flash {
			product_name = <11>; /* FLASH_GPIO */
			flash_first_gpio = <2>; /* DICO not use first, second gpio value */
			flash_second_gpio = <3>;
		};

		preprocessor {
			product_name = <2>; /* 73c2 */
			spi_channel = <1>;
			i2c_addr = <0x7a>;
			i2c_ch = <0>; /* i2c 0*/
			dma_ch = <2>; /* dma vc2*/
		};
	};

	fimc_is_sensor_4e6: fimc-is_sensor_4e6@6A {
		compatible = "samsung,sensor-module-4e6";

		pinctrl-names = "pin0", "pin1", "pin2";
		pinctrl-0 = <>;
		pinctrl-1 = <&fimc_is_mclk1_in &fimc_is_mclk2_in>;
		pinctrl-2 = <&fimc_is_mclk1_fn &fimc_is_mclk2_fn>;

		position = <1>;	/* Rear:0. Front:1 */
		id = <0>; /* sensor node id */
		mclk_ch = <1>;
		sensor_i2c_ch = <1>;
		sensor_i2c_addr = <0x6A>;

		gpio_mclk = <&gpe0 1 0x1>;
		gpio_reset = <&gpd4 6 0x1>;
		gpio_standby = <&gpa1 5 0x1>;
		gpio_prep_reset = <&gpb0 0 0x1>; /* preprocessor reset */
		status = "okay";

		preprocessor {
			product_name = <2>; /* 73c2 */
			spi_channel = <1>;
			i2c_addr = <0x7a>;
			i2c_ch = <0>; /* i2c 0*/
			dma_ch = <2>; /* dma vc2*/
		};


	};
	/* SENSORHUB */
	spi_2: spi@13930000 {
        #address-cells = <1>;
        #size-cells = <0>;

		pinctrl-names = "default";
		pinctrl-0 = <&spi2_mosi_sck_ssn &spi2_miso>;

		num-cs = <1>;
		status = "okay";

		BCM4773@0 {
			compatible = "ssp,BCM4773";
			reg = <0>;
			spi-max-frequency = <26000000>;
			spi-cpol;
			spi-cpha;
			pinctrl-names = "default";
			pinctrl-0 = <&ssp_host_req &ssp_mcu_req &ssp_mcu_resp>;

			gpio-controller;
            #gpio-cells = <2>;
			ssp-host-req = <&gpa0 1 0x01>;
			ssp-mcu-req = <&gpa3 0 0x01>;
			ssp-mcu-resp = <&gpb0 3 0x00>;
			ssp-acc-position = <0>;
			ssp-mag-position = <0>;
			ssp-sns-combination = <0>;
			ssp,prox-hi_thresh = <2000>;
			ssp,prox-low_thresh = <1400>;
			ssp-ap-rev = <1>;
			ssp-mag-array = <10227 (-404) 215 (-666) 8429 (-61) 853 (-1154) 11138>; /* Only for YAS532 */
			ssp-glass-type = <0>;

			controller-data {
				cs-gpio = <&gpc4 3 0>;
				samsung,spi-feedback-delay = <0>;
				};
			};
	};
	/* GPS */
	gps {
		compatible = "samsung,exynos54xx-bcm4753";

		pinctrl-names = "default";
		pinctrl-0 = <&ssp_gps_pwr_en>;

		gpios = <&gpd4 5 0x1>;
		status = "okay";
	};

        pinctrl@139B0000 {
            /*mcu_resp rev 0.0*/
    	    ssp_mcu_resp: ssp-mcu-resp {
    		samsung,pins = "gpb0-3";
    		samsung,pin-function = <0>;
    		samsung,pin-pud = <3>;
    		samsung,pin-drv = <3>;
    	};
    };
    tmuctrl_2: tmuctrl_G3D@10130000 {
		/* flags, driver_data(index), frequency */
		gpu_cooling_table = < 0 0 1246000
				      0 1 1100000
				      0 2 962000
				      0 3 728000
				      0 4 350000
				      0 5 TABLE_END>;
	};

	decon_board_list: decon_board_list {
		dsim_set_panel_power_enable {
			type = "regulator,enable", "gpio,high", "regulator,enable";
			regulator = "LCD_1P8","LCD_3P0";
			gpios = <&gpb0 1 0x1>;
		};
		dsim_set_panel_power_disable {
			type = "gpio,low", "regulator,disable", "gpio,low", "regulator,disable";
			regulator = "LCD_1P8","LCD_3P0";
			gpios = <&gpd3 4 0x1>, <&gpb0 1 0x1>;
		};
		dsim_reset_panel {
			type = "gpio,high", "delay,usleep", "gpio,low", "delay,usleep", "gpio,high", "delay,usleep";
			gpios = <&gpd3 4 0x1>, <&gpd3 4 0x1>, <&gpd3 4 0x1>;
			delay = <5000 6000>, <5000 6000>, <10000 11000>;
		};
	};

	sound {
		compatible = "samsung,universal7880-cod3026x";
		mic-bias-mode = <3 1 2 0>;
		mic-bias-gpios = <&gpd4 2 0>;
	};

	isa1000 {
		compatible = "isa1000";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm_tout0>;
		isa1000_pdata {
			status = "okay";
			isa1000,max_timeout = <10000>;
			isa1000,pwm_use = <1>;
			isa1000,duty = <34290>;
			isa1000,period = <38100>;
			isa1000,pwm_id = <0>;
			isa1000,gpio_en = <&gpd1 1 0>;
			isa1000,regulator_name = "VDD_MOTOR_3P0";
		};
	};
};
